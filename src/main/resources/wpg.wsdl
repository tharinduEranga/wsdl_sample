                  xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
    <wsdl:types>
        <xsd:schema targetNamespace="http://www.wileypay.com/wpg.xsd">
            <xsd:element name="vendorID" type="xsd:int">
                <xsd:annotation>
                    <xsd:documentation>Contains vendor allocated identity number.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="transID" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>Contains the vendor specified transaction number that specifically refers to this
                        current transaction.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="userID" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>Contains the user name used to cross reference for permission to use the PED
                        device.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="PEDIP" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>Contains the IP address of the agents work station to which the agent's PED
                        device is connected.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="method" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>Contains the method with which to process this transation. Must always be set to
                        'SOAP'. (RESERVED FOR FUTURE USE)
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="customerPresent" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>Contains the where this transaction is with the customer present or not.
                        (RESERVED FOR FUTURE USE)
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="timestamp" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>Contains the time stamp of the number if seconds since the Epoch (00:00:00 on
                        01/01/1970).
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="region" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>Contains the region the vendor would like the transaction settle through.
                        Presently only EMEA is available and MUST be passed. (RESERVED FOR FUTURE USE)
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="mode" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>Contains the mode of which to perform a probe on the WPG.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="type" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>Contains the type of probe to perform on the WPG.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="security" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>Contains the security countermeasure to prevent tampering with data during
                        transmission.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="returnCode" type="xsd:int">
                <xsd:annotation>
                    <xsd:documentation>Contains the WPG return code for this transaction.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="returnMessage" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>Contains the WPG return message for this transaction.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="accessToken" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>Contains the access token required for subsequent use of the PED device. Only
                        obtained from Probe function.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="description" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>Contains the vendor specific details to do with this transaction. This is not
                        typically shown to the customer.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="value" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>Contains the value that the vendor wishes to transact with the customer.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="currency" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>Contains the currency the vendor would like to transact with the customer.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="allowCSCFail" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>Contains a flag to signal that the vendor is willing to accept an Card Security
                        Check (CSC) failure.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="allowAVSFail" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>Contains a flag to signal that the vendor is willing to accept an Address
                        Verification System (AVS) failure.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="merchantResponse" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>Contains a message from the merchant service regarding the transaction.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="merchantID" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>Contains the merchant ID number used for this transaction.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="CSCResult" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>Contains the result of the Card Verification Check for this transaction.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="AVSPostResult" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>Contains the results of the post code portion of the Address Verification System
                        check for this transaction.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="AVSAddrResult" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>Contains the results of the address numerics portion of the Address Verification
                        System check for this transaction.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="token" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>Contains the tokenised version of the payment card presented by the user.
                        Contains the first 6 and last 4 digits of the REAL card number with the remainder substituded
                        with random digits.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="authCode" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>Contains the authorisation code provided by the acquiring bank for this
                        transaction.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="acquirerID" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>Contains the single letter identifier for the acquiring bank.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="acquirerName" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>Contains the name identifier for the acquiring bank.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="bankID" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>Contains the single letter identifier for the merchant bank.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="bankName" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>Contains the name identifier for the merchant bank.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="maskedCardNumber" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>Contains the obfusicated payment card number for use on reciepts.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="cardExpiry" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>Contains the payment card expiry date (MMYY).</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="cardStart" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>Contains the payment card start date (MMYY).</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="cardPAN" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>Contains the payment card number, used for tokenisation only.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="AVSPost" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>Contains the numeric elements of the customers address .</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="AVSAddr" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>Contains the numeric elements of the customers post code.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="obtained" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>Contains the result of whether a telephone authorisation has been obtained.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="countryCode" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>Contains the ISO country code of the customer.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="merchantReference" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>Contains reference supplied by merchant.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="authTransID" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>For settle transactions this is the authorisation transaction ID
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="orderId" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>This is the Order Id generated in Hybris</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="operation" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>The Wiley Payment Gateway Operation Type</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:schema>
